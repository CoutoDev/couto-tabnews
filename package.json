{
  "name": "mage-news",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:restart && next dev",
    "services:start": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml down",
    "services:restart": "npm run services:stop && npm run services:start",
    "build": "next build",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m infra/migration --envPath .env.development down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.4",
    "jest": "^29.6.2",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
